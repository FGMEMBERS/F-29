<?xml version="1.0"?>

<PropertyList>
	
<animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>1000</max-m>
    </animation>


<!--station left 1 -->

<model>
<name>left-aim9-pos-1</name>
<path>Aircraft/FGUK-weapons/Air-Air/aim9/AIM-9.xml</path>
<offsets>
<x-m>-1.1</x-m>
<y-m>-4.6</y-m>
<z-m>-0.1</z-m>
<pitch-deg>-3</pitch-deg>
</offsets>
</model>	
  
<animation>
<type>select</type>
<object-name>left-aim9-pos-1</object-name>      
<condition>
<and>
<equals>
<property>sim/weight[0]/selected</property>
<value>AIM-9 Sidewinder</value>
</equals>
<equals>
<property>controls/armament/station[0]/release</property>
<value>false</value>
</equals>
</and>
</condition>
</animation> 



